{
  "name": "sagetest",
  "version": "0.1.27",
  "description": "A simple, fast, and easy to use HTTP server testing library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddienubes/sagetest.git"
  },
  "homepage": "https://github.com/eddienubes/sagetest#readme",
  "author": "Eddie Nubes <cool.gegeg@gmail.com>",
  "bugs": {
    "url": "https://github.com/eddienubes/sagetest/issues"
  },
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "build"
  ],
  "main": "build/index.js",
  "exports": {
    ".": {
      "import": "./build/index.mjs",
      "require": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "scripts": {
    "prepack": "misc/package.sh",
    "test:all": "pnpm vitest run",
    "test:watch": "pnpm vitest watch",
    "test:ci": "pnpm vitest run --coverage",
    "build:dev": "tsc -p tsconfig.json",
    "build:prod": "rm -rf ./build && pnpm rollup -c && tsc -p tsconfig.build.json && pnpm tsc-alias -p tsconfig.build.json",
    "lint:check": "eslint --max-warnings=50 .",
    "lint:fix": "eslint --max-warnings=50 --fix .",
    "valid": "pnpm lint:fix && pnpm build:dev"
  },
  "keywords": [
    "http",
    "testing",
    "express",
    "fastify",
    "https",
    "server",
    "supertest",
    "superagent",
    "axios",
    "undici",
    "multipart",
    "test",
    "jest",
    "vitest",
    "mocha",
    "sagetest"
  ],
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^10.0.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/multipart": "^9.4.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitest/coverage-v8": "^4.0.18",
    "compare-versions": "^6.1.1",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "express": "^5.2.1",
    "fastify": "^5.7.4",
    "mime-db": "^1.54.0",
    "multer": "2.0.2",
    "prettier": "^3.8.1",
    "rollup": "^4.59.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "supertest": "^7.2.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "form-data": "^4.0.5",
    "undici": "^7.22.0"
  },
  "packageManager": "pnpm@10.30.2+sha512.36cdc707e7b7940a988c9c1ecf88d084f8514b5c3f085f53a2e244c2921d3b2545bc20dd4ebe1fc245feec463bb298aecea7a63ed1f7680b877dc6379d8d0cb4"
}
