{
  "name": "sagetest",
  "version": "0.0.1",
  "description": "A simple, fast, and easy to use HTTP server testing library",
  "repository": "https://github.com/eddienubes/sagetest",
  "homepage": "https://github.com/eddienubes/sagetest#readme",
  "author": "Eddie Nubes <cool.gegeg@gmail.com>",
  "bugs": {
    "url": "https://github.com/eddienubes/sagetest/issues"
  },
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.5.0"
  },
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    },
    "./*": {
      "import": "./build/esm/*.js",
      "require": "./build/cjs/*.js"
    }
  },
  "scripts": {
    "test:all": "pnpm vitest run",
    "test:watch": "pnpm vitest watch",
    "test:ci": "pnpm vitest run --coverage",
    "build:dev": "tsc -p tsconfig.json",
    "build:prod": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "lint:check": "eslint --max-warnings=50 .",
    "lint:fix": "eslint --max-warnings=50 --fix ."
  },
  "keywords": [
    "http",
    "testing",
    "express",
    "fastify",
    "https",
    "server",
    "supertest",
    "superagent",
    "axios",
    "undici",
    "multipart",
    "test",
    "jest",
    "vitest",
    "mocha",
    "sagetest"
  ],
  "devDependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/multipart": "^8.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "compare-versions": "^6.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "fastify": "^4.26.0",
    "form-data": "^4.0.0",
    "multer": "1.4.5-lts.1",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "undici": "^6.6.2"
  }
}
